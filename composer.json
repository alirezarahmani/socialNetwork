{
  "name": "socialnetwork/alireza",
  "type": "metapackage",
  "description": "Social network Development Environment",
  "homepage": "https://github.com/alirezarahmani/socialNetwork",
  "config": {
    "process-timeout": 2000
  },
  "require-dev": {
    "phpunit/phpunit": "6.0.*",
    "squizlabs/php_codesniffer": "3.*",
    "ext-pdo": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "SocialNetwork\\": "src/",
      "Boot\\": "boot/",
      "Test\\": "tests/"
    }
  },
  "require": {
    "mockery/mockery": "^1.1",
    "beberlei/assert": "^3.0",
    "symfony/dependency-injection": "^4.1",
    "symfony/yaml": "^4.1",
    "symfony/http-foundation": "^4.1",
    "symfony/event-dispatcher": "^4.1",
    "symfony/routing": "^4.1",
    "symfony/validator": "^4.1",
    "symfony/console": "^4.1",
    "prooph/service-bus": "^6.2",
    "prooph/event-sourcing": "^5.0",
    "ramsey/uuid": "^3.8",
    "prooph/pdo-event-store": "^1.9",
    "prooph/standard-projections": "^1.1"
  }
}
